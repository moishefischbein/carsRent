<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.moish.carrentforcompany.controller.MainActivity">


    <LinearLayout         android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">
        <Button
            android:id="@+id/addStudentButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add Student" />
        <Button
            android:id="@+id/addLecturerButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add Lecturer" />
        <Button
            android:id="@+id/addCourseButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add Course" />
        <Button
            android:id="@+id/addStudentCourseButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add StudentCourse" />
        <Button
            android:id="@+id/showStudentListButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:text="Show Student List" />
        <Button
            android:id="@+id/showCourseListButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Show Course List" />
    </LinearLayout>


</android.support.constraint.ConstraintLayout>
